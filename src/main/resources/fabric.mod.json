{
	"schemaVersion": 1,
	"id": "composite",
	"version": "${version}",
	"name": "Composite",
	"description": "A library mod that lets you use Jetpack Compose and Material 3 in Minecraft.",
	"authors": [
		"apersomany"
	],
	"contact": {
		"homepage": "https://erudites.dev",
		"sources": "https://github.com/erudites-dev/composite"
	},
	"license": "All Rights Reserved",
	"environment": "client",
	"entrypoints": {
		"client": [
			{
				"value": "dev.aperso.composite.Composite",
				"adapter": "kotlin"
			}
		]
	},
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	}
}